        <BS_BananaSplitSlimeGiant MayRequire="RedMattis.BSSlimes">0.00</BS_BananaSplitSlimeGiant>
      </xenotypeChances>
    </xenotypeSet>
    <raidCommonalityFromPointsCurve>
