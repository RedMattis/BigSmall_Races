        <BS_FireJotun>0.0</BS_FireJotun>
        <BS_Surtr>0.0</BS_Surtr>
        <BS_BrokenTitan>0.0</BS_BrokenTitan>
        <BS_FrostSlimeGiant MayRequire="RedMattis.BSSlimes">0.00</BS_FrostSlimeGiant>
